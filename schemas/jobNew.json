{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/jobNew.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "company_handle": {
      "type": "string"
    },
    "salary": {
      "type": "integer"
    },
    "equity": {
      "type": "number"
    }
  },
  "additionalProperties": false,
  "required": ["title", "salary", "equity", "company_handle"]
}
